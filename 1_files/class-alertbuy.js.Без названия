// базы данных, заполняется вручную по примеру
var appAlertBuy_baza_1 = {
	// мужские имена
	men_name: ["Александр","Андрей","Антон","Арсен","Артем","Богдан","Борис","Вадим","Валентин","Валерий","Василий","Виктор","Виталий","Владимир","Владислав","Вячеслав","Геннадий","Глеб","Григорий","Давид","Даниил","Денис","Дмитрий","Евгений","Егор","Иван","Игорь","Илья","Кирилл","Константин","Леонид","Макар","Максим","Марк","Матвей","Михаил","Никита","Николай","Олег","Павел","Петр","Роман","Ростислав","Руслан","Сергей","Тимофей","Федор","Юрий","Ярослав",],
